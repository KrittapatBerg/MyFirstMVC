@{
    ViewData["Title"] = "Quan's Page";
}

<div class="text.center">

    <h1 class="display-4"> Quan Page </h1>
    <p>Eating Noodle with Kimchee and waving: @Model.Greetings</p>

    <p>
        <div class="row mb-2 text-center">
            <div class="col-md-12 themed-grid-head-col">Latin sentences</div>
        </div>

        <div class="row mb-2 text-center">
            @foreach (var item in Model.Latin)
            {
                <div class="col-md-12 themed-grid-col">@item.Sentence</div>
            }
        </div>
    </p>
</div>

<div class="row g-1">
    <h3 class="pb-2">Artists</h3>

    <div class="row mb-2 text-center">
        <div class="col-md-12 themed-grid-head-col">Name</div>
    </div>
    <div class="row mb-2 text-center">
        <div class="col-md-12 themed-grid-col">Angus Young</div>
    </div>
</div>
